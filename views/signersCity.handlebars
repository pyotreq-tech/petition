
<div class="petition-container">

    {{>content}}

    <div class="petition-container-right">

        

        <div class="petition-div-item">
        <h2 class="petition-font-left">Looks like we have some support in {{city}}:</h2>
        </div>



        {{#rows}}
        <div class="petition-div-item-display">


        {{#if url}}
            <div class="petition-div-item-display-item link">
                <a href="{{url}}" target="_blank" class="petition-p-signers-link" >{{first}} {{last}} </a>
            </div>
        {{else}}
            <div class="petition-div-item-display-item">        
                <span class="petition-p-signers">{{first}} {{last}}</span>
            </div>
        {{/if}}
        

        {{#if age}}
            <div class="petition-div-item-display-item">
                 <span class="petition-p-signers">{{age}} </span>                
            </div>
        {{/if}}
        </div>
        {{/rows}}
</div>
   
</div>



